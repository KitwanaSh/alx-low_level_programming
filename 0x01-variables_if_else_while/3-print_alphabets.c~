#include <stdio.h>
#include <ctype.h>

/**
 * Print the alphabets from a to z
 *
 * Then print in lowercase and upper case
 */
int main() /* Thi is it*/
{
  char c = 'a';

  while (c <= 'z')
    {
      putchar(c);
      c++;
    }
  while (c <= 'z')
    {
      putchar(toupper(c));
      c++;
    }
  putchar('\n');

  return (0);
}
